<form method="post" action="{{ path('campus_delete', {'id': campus.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ campus.id) }}">
    <button class="btn btn-primary">Supprimer</button>
</form>
