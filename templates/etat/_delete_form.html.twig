<form method="post" action="{{ path('etat_delete', {'id': etat.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ etat.id) }}">
    <button class="btn btn-primary">Supprimer</button>
</form>
